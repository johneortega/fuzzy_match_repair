						</equal>
					</test>
					<let>
						<var n="v_tense"/>
						<lit-tag v="pri"/>
					</let>
				</when>
				<when>
					<test>
						<equal>
							<clip pos="1" side="tl" part="a_tense"/>
							<lit-tag v="past"/>
						</equal>
					</test>
					<let>
						<var n="v_tense"/>
						<lit-tag v="pii"/>
					</let>
				</when>
				<otherwise>
					<let>
						<var n="v_tense"/>
						<clip pos="1" side="tl" part="a_tense"/>
					</let>
				</otherwise>
			</choose>
		</def-macro>


