                  <test>
                    <equal caseless="yes">
                      <clip pos="2" side="sl" part="lem"/>
                      <lit v="agazapar"/>
                    </equal>
                  </test>
                  <let>
                    <clip pos="2" side="tl" part="lem"/>
                    <lit v="tapir"/>
                  </let>
                </when>
                <when>
                  <test>
                    <equal caseless="yes">
                      <clip pos="2" side="sl" part="lem"/>
                      <lit v="apelotonar"/>
                    </equal>
                  </test>
                  <let>
                    <clip pos="2" side="tl" part="lem"/>
