                      <lit v="estrenar"/>
                    </equal>
                  </test>
                  <let>
                    <clip pos="2" side="tl" part="lem"/>
                    <lit v="débuter"/>
                  </let>
                </when>
                <when>
                  <test>
                    <equal caseless="yes">
                      <clip pos="2" side="sl" part="lem"/>
                      <lit v="concienciar"/>
                    </equal>
                  </test>
                  <let>
                    <clip pos="2" side="tl" part="lem"/>
                    <lit v="prendre# conscience"/>
                  </let>
                </when>
                <when>
                  <test>
                    <equal caseless="yes">
                      <clip pos="2" side="sl" part="lem"/>
                      <lit v="aficionar"/>
                    </equal>
                  </test>
                  <let>
                    <clip pos="2" side="tl" part="lem"/>
                    <lit v="prendre# goût"/>
                  </let>
                </when>
