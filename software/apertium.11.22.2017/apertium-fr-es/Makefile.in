install-apertium_fr_esDATA: $(apertium_fr_es_DATA)
	@$(NORMAL_INSTALL)
	@list='$(apertium_fr_es_DATA)'; test -n "$(apertium_fr_esdir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(apertium_fr_esdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(apertium_fr_esdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(apertium_fr_esdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(apertium_fr_esdir)" || exit $$?; \
	done

uninstall-apertium_fr_esDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(apertium_fr_es_DATA)'; test -n "$(apertium_fr_esdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(apertium_fr_esdir)'; $(am__uninstall_files_from_dir)

